<h1>New Assignment</h1>
<%= form_for ([@course, @assignment]) do |form| %>

  <p>
    <%= form.label :name, "Name" %>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :due, "Due Date" %>
    <%= form.datetime_local_field :due %>
  </p>

  <p class='buttons'>
    <%= form.submit "Create Assignment" %>
  </p>

<% end %>